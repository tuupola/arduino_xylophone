<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>Püha Taevas</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link type="text/css" rel="stylesheet" href="css/base.css" media="screen" />
<!--[if lte IE 6]>
<style type="text/css">

img {
behavior:url("inc/iepngfix.htc");
}

</style>
<![endif]--> 
</head>
<body class="home">

  <div id="taevas">
    <p><a href="http://www.taevas.ee"><span>www.taevas.ee</span></a></p>
  </div>

  <!-- #container start -->
  <div id="container">
  <div id="containerSpan">

    <!-- #header start -->
    <div id="header">

      <h1><a href="/"><span>Püha Taevas</span></a></h1>
      <div id="intro">
        <h2>“<%= @song.name %>”</h2>
      </div>

    </div>
    <!-- #header end -->

    <!-- #content start -->
    <div id="content">

      <div id="video">
        <div id="videoSpan" title="<%= @song.id %>">
        </div>
      </div>

      <div id="menu">
      <div id="menuSpan">
        <p>
          <a href="/song" id="lCreate"><span>Astu edasi ja loo oma lugu Taeva Pühal Instrumendil.</span></a>
          <a href="/random" id="lOthers"><span>Inspireeru teistest valgustatuist.</span></a>
        </p>
      </div>
      </div>

      <div class="clearer"></div>

    </div>
    <!-- #content end -->

  </div>
  </div>
  <!-- #container end -->
  
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.min.js" type="text/javascript" charset="utf-8"></script>  
  <script src="/js/flowplayer-3.1.4.min.js" type="text/javascript" charset="utf-8"></script>
  <script src="/js/soundmanager2.js" type="text/javascript" charset="utf-8"></script>
  <script src="/js/application.js" type="text/javascript" charset="utf-8"></script>

</body>
</html>